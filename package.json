{
  "version": "0.0.0-development",
  "name": "eslint-config-dacz-node",
  "description": "Shareable eslint config for node by dacz",
  "author": "David Cizek <david@dchere.com> (https://twitter.com/dadc)",
  "homepage": "https://github.com/dacz/eslint-config-dacz-node",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dacz/eslint-config-dacz-node.git"
  },
  "bugs": {
    "url": "https://github.com/dacz/eslint-config-dacz-node/issues"
  },
  "readme": "README.md",
  "directories": {},
  "keywords": [
    "eslint",
    "prettier",
    "codestyle",
    "node",
    "flow"
  ],
  "scripts": {
    "cm": "git-cz",
    "cm:retry": "git-cz --retry",
    "lint": "eslint .",
    "test": "npm run --silent lint",
    "eslint-check": "eslint --print-config index.js | eslint-config-prettier-check",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "dependencies": {
    "babel-eslint": "8.0.3",
    "eslint": "^4.13.0",
    "eslint-plugin-ava": "^4.4.0",
    "eslint-plugin-flowtype": "^2.40.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-security": "^1.4.0"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.3.1",
    "prettier": "^1.9.1",
    "semantic-release": "^8.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
